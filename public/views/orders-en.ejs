    <ul class="list-group list-group-horizontal">
      <li class="list-group-item">
        Order ID: <%= data[i].order_id %>
      </li>
      <li class="list-group-item">
        DOD: <%= data[i].DOD %>
      </li>
      <li class="list-group-item">
        DOE: <%= data[i].DOO %>
      </li>
      <li class="list-group-item">
        Item ID: <%= data[i].item_id %>
      </li>
      <li class="list-group-item">
        Units: <%= data[i].units %>
      </li>
      <% if(data[i].status === 1) { %>
      <li class="list-group-item"> Status: Completed </li>
      <% } else { %>
      <li class="list-group-item"> Status: Incomplete </li>
      <% } %>
